{
  "name": "puppet-etcd",
  "version": "2.0.0",
  "author": "Vox Pupuli",
  "summary": "Etcd Puppet Module",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-etcd",
  "project_page": "https://github.com/voxpupuli/puppet-etcd",
  "issues_url": "https://github.com/voxpupuli/puppet-etcd/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    },
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 5.0.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Rocky",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "9"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ]
}
